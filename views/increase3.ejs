<script>
    $("document").ready(function () {
        var callback = function (data) {
            console.log("callback");
            $('#myLabel').html(data);
        };

        $("#myButton").click(function () {
            $.get('/increase3ZHE', {value: $('#myLabel').text()}, callback);
        });
    });
</script>

<label id="myLabel">0</label>
<button id="myButton">Click me</button>